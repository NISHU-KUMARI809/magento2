<?xml version="1.0"?>
<config 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Customer\Model\Customer">
        <plugin name="nishu_customer_plugin" type="Nishu\HelloWorld\Plugin\CustomerPlugin" sortOrder="1"/>
    </type> 
    <type name="Magento\Shipping\Model\CarrierFactory">
<arguments>
<argument name="carriers" xsi:type="array">
<item name="helloworld" xsi:type="string">Nishu\HelloWorld\Model\Carrier\Shipping</item>
</argument>
</arguments>
</type>
<preference for="Nishu\HelloWorld\Api\CustomInterface" type="Nishu\HelloWorld\Model\Api\Custom"/>
</config>
 
